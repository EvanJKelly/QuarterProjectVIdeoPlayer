@{
    ViewData["Title"] = "Home Page";
}





    
        <div class="text-center">
            <h1 style="color: deepskyblue" class="display-4">Welcome to our Video Player Site!</h1>
            <h2 style="color: deepskyblue;"> Search a video or two and enjoy!</h2>
<body>
    
    
    <div class="text-center">
        <h1 style="color: deepskyblue" class="display-4">Welcome to our Video Player Site!</h1>
        <h2 style="color: deepskyblue" > Search a video or two and enjoy!</h2>
        </div>
    <div style="position:absolute; z-index:-1; margin-left:50px;">
        <video id='SampleVideos/SampleVideo.mp4' class='video-js' controls preload='auto' width='250' height='150'
               poster='SampleVideos/shrek.jfif' data-setup='{}'>
            <source src='SampleVideos/SampleVideo.mp4' type='video/mp4'>
            <source src='SampleVideos/SampleVideo.webm' type='SampleVideo/webm'>
            <p class='vjs-no-js'>
                To view this video please enable JavaScript, and consider upgrading to a web browser that
                <a href='https://videojs.com/html5-video-support/' target='_blank'>supports HTML5 video</a>
            </p>


        </div>
        <video id="my-player"
               class="video-js"
               controls
               preload="auto"
               poster="~/SampleVideos/shrek.jfif"
               data-setup='{}' width="235" height="150">
            <source src="~/SampleVideos/SampleVideo.mp4" type="video/mp4"></source>
            <source src="//vjs.zencdn.net/v/oceans.webm" type="video/webm"></source>
            <source src="//vjs.zencdn.net/v/oceans.ogv" type="video/ogg"></source>
            <p class="vjs-no-js">
                To view this video please enable JavaScript, and consider upgrading to a
                web browser that
                <a href="http://videojs.com/html5-video-support/" target="_blank">
                    supports HTML5 video
                </a>
            </p>
        </video>
        <script src='https://vjs.zencdn.net/7.6.5/video.js'></script>
    

        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    </div>
}

    </body>